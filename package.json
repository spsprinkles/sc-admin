  },
  "homepage": "https://github.com/datta-framework/sc-admin#readme",
  "dependencies": {
    "datatables.net": "^1.13.1",
    "datatables.net-bs5": "^1.13.1",
    "dattatable": "^2.6.7",
    "gd-sprest-bs": "^10.10.10",
    "jquery": "^3.6.1",
    "moment": "^2.29.4",
    "text-encoding": "^0.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@babel/preset-env": "^7.20.2",
    "babel-loader": "^9.1.0",
    "css-loader": "^6.7.2",
